# 风控系统配置文件示例

# 代理池配置
PROXY_POOL_ENABLED=true
PROXY_BINDING_STRATEGY=sticky  # sticky, random, round_robin, least_used

# 速率限制配置
RATE_LIMIT_ENABLED=true
GLOBAL_RPM=1000  # 全局每分钟请求数
GLOBAL_TPM=1000000  # 全局每分钟Token数
ACCOUNT_RPM=60  # 单账号每分钟请求数
ACCOUNT_TPM=90000  # 单账号每分钟Token数
USER_RPM=100  # 单用户每分钟请求数
USER_TPM=150000  # 单用户每分钟Token数

# 指纹伪装配置
FINGERPRINT_ENABLED=true
STICKY_FINGERPRINT=true  # 账号绑定固定指纹

# 健康监控配置
HEALTH_MONITOR_ENABLED=true
AUTO_RECOVERY_ENABLED=true
HEALTH_CHECK_INTERVAL=60  # 健康检查间隔（秒）

# 代理健康检查
PROXY_HEALTH_CHECK_ENABLED=true
PROXY_HEALTH_CHECK_INTERVAL=300  # 代理健康检查间隔（秒）
